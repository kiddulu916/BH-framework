{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NEXT_PRIVATE_DISABLE_LIGHTNINGCSS=1 next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "analyze": "ANALYZE=true npm run build",
    "analyze:dev": "ANALYZE=true npm run dev",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "build:prod": "npm run build",
    "build:dev": "cross-env NODE_ENV=development npm run build",
    "docker:build": "DOCKER_ENV=true npm run build",
    "docker:start": "DOCKER_ENV=true npm run start",
    "docker:dev": "DOCKER_ENV=true npm run dev",
    "health": "curl -f http://localhost:3000/health || exit 1"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/react-query": "^5.83.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "critters": "^0.0.23",
    "dotenv": "^17.2.0",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.525.0",
    "next": "^14.2.30",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^4.1.11",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.3",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.4.2",
    "jsdom": "^24.0.0",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
